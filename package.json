{
  "name": "arc3dlab",
  "version": "0.0.4",
  "main": "dist/arc3dlab.esm.js",
  "module": "dist/arc3dlab.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "build:copy": "rollup -c && xcopy /Y \"dist\\arc3dlab.umd.js\" \"demo-html\\public\\lib\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "node scripts/generate-docs.js",
    "docs:watch": "typedoc --options typedoc.json --watch",
    "docs:build": "typedoc --options typedoc.json",
    "dev": "node scripts/dev-setup.js",
    "dev:watch": "node scripts/dev-setup.js --watch"
  },
  "keywords": [
    "3D",
    "webGL",
    "WebGis",
    "Cesium",
    "Arc3DLab",
    "Arc3DLab SDK"
  ],
  "author": "arc3dlab",
  "license": "ISC",
  "meta": {
    "nodejsVersion": "v22.20.0"
  },
  "description": "基于开源项目 Cesium 封装的 WebGis 应用框架",
  "peerDependencies": {
    "cesium": "1.136.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.1",
    "rollup": "^2.79.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typedoc": "^0.28.15",
    "typedoc-material-theme": "^1.4.1",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/*",
    "README.md",
    "CHANGES.md",
    "LOG.md"
  ]
}
